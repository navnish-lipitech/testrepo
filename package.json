{
  "name": "@lipihipi/rtc-knowledgebank",
  "version": "0.0.7",
  "description": "RTC knowledge bank",
  "author": "Navnish Singh",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite",
    "start": "vite build --watch",
    "build": "tsc --p ./tsconfig.build.json && vite build --no-watch",
    "storybook": "storybook dev -p 6006 public",
    "build-storybook": "storybook build -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@storybook/test": "8.0.5",
    "dayjs": "^1.11.10",
    "tslib": "^2.6.2",
    "yup": "^1.3.2"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "@lipihipi/rtc-client-sdk": "0.0.30",
    "@lipihipi/rtc-ui-components": "^0.0.29",
    "@storybook/addon-actions": "^8.0.5",
    "@storybook/addon-essentials": "^8.0.5",
    "@storybook/addon-interactions": "^8.0.5",
    "@storybook/addon-links": "^8.0.5",
    "@storybook/node-logger": "^8.0.5",
    "@storybook/preset-create-react-app": "^8.0.5",
    "@storybook/react": "^8.0.5",
    "@storybook/react-vite": "^8.0.5",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.43",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^4.2.1",
    "babel-plugin-named-exports-order": "^0.0.2",
    "postcss": "^8.4.14",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^8.0.5",
    "typescript": "^4.7.4",
    "sass": "^1.72.0",
    "vite": "^5.2.6",
    "vite-plugin-css-injected-by-js": "^3.5.0",
    "vite-plugin-dts": "^3.7.3",
    "vite-tsconfig-paths": "^4.3.2",
    "webpack": "^5.73.0"
  },
  "peerDependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@lipihipi/rtc-ui-components": "^0.0.22",
    "@mui/icons-material": "^5.14.9",
    "@mui/material": "^5.14.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
